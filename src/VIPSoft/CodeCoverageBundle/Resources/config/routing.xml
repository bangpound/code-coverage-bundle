<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="VIPSoft_CodeCoverageBundle_CodeCoverage_Create" pattern="/">
       <default key="_controller">VIPSoftCodeCoverageBundle:CodeCoverage:create</default>
       <requirement key="_method">POST</requirement>
    </route>

    <route id="VIPSoft_CodeCoverageBundle_CodeCoverage_Read" pattern="/">
       <default key="_controller">VIPSoftCodeCoverageBundle:CodeCoverage:read</default>
       <requirement key="_method">GET</requirement>
    </route>

    <route id="VIPSoft_CodeCoverageBundle_CodeCoverage_Delete" pattern="/">
       <default key="_controller">VIPSoftCodeCoverageBundle:CodeCoverage:delete</default>
       <requirement key="_method">DELETE</requirement>
    </route>

</routes>
